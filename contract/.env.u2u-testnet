# U2U Nebulas Testnet Configuration
# Copy this to .env when deploying to U2U Testnet

# Network Configuration
NETWORK_NAME=u2uTestnet
CHAIN_ID=2484
U2U_TESTNET_RPC_URL=https://rpc-nebulas-testnet.u2u.xyz

# Deployer Configuration
DEPLOYER_PRIVATE_KEY=0x_your_deployer_private_key_here

# Contract Configuration (set after deployment)
CONTRACT_ADDRESS=

# Server Configuration
SERVER_SIGNER_ADDRESS=0x_your_server_signer_address_here
FEE_RECIPIENT_ADDRESS=0x_your_fee_recipient_address_here

# Explorer
U2U_EXPLORER_API_KEY=not-needed

# Gas Reporting
REPORT_GAS=false
